<?xml version="1.0"?>
<opencv_storage>
<Settings>

<!-- Number of inner corners per a item row and column. (square, circle) -->
<landMarkShapeOrigin>4</landMarkShapeOrigin>
<landMarkShapeReference>4</landMarkShapeReference>
<!-- Number of inner corners per a item row and column. (square, circle) -->
<landMarkColorOriginMin>0</landMarkColorOriginMin>
<landMarkColorOriginMax>127</landMarkColorOriginMax>
<landMarkColorReferenceMin>128</landMarkColorReferenceMin>
<landMarkColorReferenceMax>255</landMarkColorReferenceMax>
<!-- Number of inner corners per a item row and column. (square, circle) -->
<landMarkSizeOrigin>10</landMarkSizeOrigin>
<landMarkSizeReference>10</landMarkSizeReference>
<!-- Number of inner corners per a item row and column. (square, circle) -->
<objectShape>3</objectShape>
<objectColorMin>0</objectColorMin>
<objectColorMax>127</objectColorMax>
<objectSizeCM>10</objectSizeCM>
<!-- Number of inner corners per a item row and column. (square, circle) -->
<harrisCornerBlockSize>4</harrisCornerBlockSize>
<harrisCornerApertureSize>3</harrisCornerApertureSize>
<harrisCornerVariance>0.1</harrisCornerVariance>
<harrisCornerThresh>180</harrisCornerThresh>
<harrisCornerMergeArea>3</harrisCornerMergeArea>
<!-- Number of inner corners per a item row and column. (square, circle) -->
<houghCircleThreshold>50</houghCircleThreshold>
<!-- Number of inner corners per a item row and column. (square, circle) -->
<gaussianStandardDeviation>3</gaussianStandardDeviation>
<gaussianSize>9</gaussianSize>




<!-- Number of inner corners per a item row and column. (square, circle) -->
<BoardSize_Width>9</BoardSize_Width>
<BoardSize_Height>6</BoardSize_Height>

<!-- The size of a square in some user defined metric system (pixel, millimeter)-->
<Square_Size>26</Square_Size>

<!-- The type of input used for camera calibration. Default: CHESSBOARD-->
<Calibrate_Pattern>"CHESSBOARD"</Calibrate_Pattern>

<!-- The input to use for calibration. To use an input camera -> give the ID of the camera, like "1"-->
<Input>"0"</Input>

<!-- How many frames to use, for calibration. -->
<Calibrate_NrOfFrameToUse>25</Calibrate_NrOfFrameToUse>

<!-- Consider only fy as a free parameter, the ratio fx/fy stays the same as in the input cameraMatrix. 
   Use or not setting. 0 - False Non-Zero - True-->
<Calibrate_FixAspectRatio>1</Calibrate_FixAspectRatio>

<!-- If true (non-zero) tangential distortion coefficients  are set to zeros and stay zero.-->
<Calibrate_AssumeZeroTangentialDistortion>1</Calibrate_AssumeZeroTangentialDistortion>

<!-- If true (non-zero) the principal point is not changed during the global optimization.-->
<Calibrate_FixPrincipalPointAtTheCenter>1</Calibrate_FixPrincipalPointAtTheCenter>
<calibPanelHeightMM>6</calibPanelHeightMM>  

<!-- The name of the output log file. -->
<Write_outputFileName>"out_camera_data.xml"</Write_outputFileName>

<!-- If true (non-zero) we write to the output file the feature points.-->
<Write_DetectedFeaturePoints>1</Write_DetectedFeaturePoints>

<!-- If true (non-zero) we write to the output file the extrinsic camera parameters.-->
<Write_extrinsicParameters>1</Write_extrinsicParameters>
	 
</Settings>
</opencv_storage>

